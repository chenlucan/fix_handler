dependences:
    boost
    sbe
    zeromq
    quickfix
    mongodb
    gtest/gmock
    protobuf
    
run zeromq receiver:(receive original mdp message for save to db on port 5557)
    ./zmq_receiver_test
    
run udp/tcp packet receiver(receive packet on port 30001-30007, and send mdp message on port 5557, send book on port 5558):
    ./udp_receiver_test -s|-j
    (s: pre-opening startup; j: later joiner startup)
    
run udp packet sender:
    ./udp_sender_test -d|-r|-i|-a <send_address> <send_port> <send_interval_ms> <lost_on>
    (d: definition message; r: market recovery message; i: increment message; a: mixed message)
    
run tcp replay packet sender:
	./tcp_sender_test <send_port>    
	
run exchange server:(listener on port 5001)
	./exchange_server_test	
	
run strategy simulater:
	./strategy_test <send_port> <result_receive_port> <book_receive_port> <send_interval_ms>
	
run exchange client:(receive from strategy from 6001, send result back on 6002, send fix command on port 5001)
	./exchange_client_test -s|-j
    (s: week begin startup; j: middle week startup)
	

